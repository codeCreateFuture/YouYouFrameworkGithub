Sys_EffectEntity = { Id = 0, Desc = "", PrefabId = 0, KeepTime = 0, SoundId = 0, Type = 0 }

Sys_EffectEntity.__index = Sys_EffectEntity;

function Sys_EffectEntity.New(Id, Desc, PrefabId, KeepTime, SoundId, Type)
    local self = { };
    setmetatable(self, Sys_EffectEntity);

    self.Id = Id;
    self.Desc = Desc;
    self.PrefabId = PrefabId;
    self.KeepTime = KeepTime;
    self.SoundId = SoundId;
    self.Type = Type;

    return self;
end